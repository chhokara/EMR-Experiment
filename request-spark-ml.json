{
  "name": "spark-ml",
  "virtualClusterId": "4kv5o1d6irbd1es3oz9z7u24g",
  "executionRoleArn": "arn:aws:iam::466175767985:role/emr-eks-spark-job-execution",
  "releaseLabel": "emr-6.14.0-latest",
  "jobDriver": {
    "sparkSubmitJobDriver": {
      "entryPoint": "s3://emr-eks-spark-us-west-2/ml-randomforest.py",
      "entryPointArguments": [
        "s3://emr-eks-spark-us-west-2/cleaned-data/",
        "s3://emr-eks-spark-us-west-2/output",
        "s3://emr-eks-spark-us-west-2/metrics"
      ],
      "sparkSubmitParameters": "--conf spark.executor.instances=2 --conf spark.executor.memory=8G --conf spark.executor.cores=2 --conf spark.driver.cores=1 --conf spark.driver.memory=8G --conf spark.executorEnv.PYSPARK_PYTHON=./numpy-env/bin/python3 --conf spark.pyspark.python=./numpy-env/bin/python3 --archives s3://emr-eks-spark-us-west-2/numpy-env.zip#numpy-env"
    }
  },
  "configurationOverrides": {
    "monitoringConfiguration": {
      "cloudWatchMonitoringConfiguration": {
        "logGroupName": "/emr-eks-spark",
        "logStreamNamePrefix": "spark-ml"
      },
      "s3MonitoringConfiguration": {
        "logUri": "s3://emr-eks-spark-us-west-2/logs/"
      }
    }
  }
}
